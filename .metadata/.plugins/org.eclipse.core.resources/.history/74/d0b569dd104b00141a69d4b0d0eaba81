package Algorithms;

import java.util.Deque;

/**
 * The Interface Algorithm.
 */
public interface Algorithm {
	
	/**
	 * Distancia.
	 *
	 * @return the int
	 */
	int distance(int reference, int val);
	
	/**
	 * Ros.
	 *
	 * @return the int
	 */
	double ros();
	
	void calculate(Deque<Integer> data, int val);
}
